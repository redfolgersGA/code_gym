
<h2>Add a cheese!</h2>





  <%= form_for @cheese do |f| %>
  <p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  </p>

  <p>
  <%= f.label :milk_type %><br>
  <%= f.text_field :milk_type %>
  </p>
  <p>
  <%= f.label :image_url %><br>
  <%= f.text_field :image_url %>
  </p>
  <p>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
  </p>


  <p>
  <%= f.submit %>
  </p>
<% end %>

